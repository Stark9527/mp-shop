.ui-clear {
  zoom: 1; }
  .ui-clear:before, .ui-clear:after {
    display: table;
    content: " "; }
  .ui-clear:after {
    clear: both; }

.ui-overflow-clip {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap; }

.ui-overflow-clip-two {
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-break: break-all; }

.ui-fl {
  float: left; }

.ui-fr {
  float: right; }

.ui-tl {
  text-align: left; }

.ui-tr {
  text-align: right; }

.ui-tc {
  text-align: center; }

.ui-tj {
  text-align: justify; }

.all-wrap .swiper {
  height: 300rpx; }
  .all-wrap .swiper .swiper-item .item-image {
    display: block;
    width: 100%;
    height: 300rpx; }
.all-wrap > .grid {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 60rpx; }
  .all-wrap > .grid > .item {
    border: 2rpx solid #e2e2e2;
    width: 180rpx;
    height: 180rpx;
    line-height: 180rpx;
    text-align: center; }
