.ui-clear {
  zoom: 1; }
  .ui-clear:before, .ui-clear:after {
    display: table;
    content: " "; }
  .ui-clear:after {
    clear: both; }

.ui-overflow-clip {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap; }

.ui-overflow-clip-two {
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-break: break-all; }

.ui-fl {
  float: left; }

.ui-fr {
  float: right; }

.ui-tl {
  text-align: left; }

.ui-tr {
  text-align: right; }

.ui-tc {
  text-align: center; }

.ui-tj {
  text-align: justify; }

.discover {
  display: inline-block;
  margin-left: 10rpx; }
  .discover > .iconfont {
    display: inline-block; }
