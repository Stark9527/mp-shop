.ui-clear {
  zoom: 1; }
  .ui-clear:before, .ui-clear:after {
    display: table;
    content: " "; }
  .ui-clear:after {
    clear: both; }

.ui-overflow-clip {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap; }

.ui-overflow-clip-two {
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-break: break-all; }

.ui-fl {
  float: left; }

.ui-fr {
  float: right; }

.ui-tl {
  text-align: left; }

.ui-tr {
  text-align: right; }

.ui-tc {
  text-align: center; }

.ui-tj {
  text-align: justify; }

page {
  background-color: #e2e2e2; }

.uploader-warp > .remark {
  box-sizing: border-box;
  width: 100%;
  height: 280rpx;
  padding: 30rpx;
  background: #ffffff; }
.uploader-warp > .uploader-box {
  margin: 20rpx 0 40rpx 0;
  padding: 30rpx;
  background: #ffffff; }
  .uploader-warp > .uploader-box > .uploader {
    display: inline-block;
    position: relative;
    margin-right: 20rpx;
    box-sizing: border-box;
    border: 2rpx solid #e2e2e2;
    width: 160rpx;
    height: 160rpx; }
    .uploader-warp > .uploader-box > .uploader::before, .uploader-warp > .uploader-box > .uploader::after {
      display: block;
      content: '';
      position: absolute;
      top: 50%;
      left: 50%;
      width: 100rpx;
      height: 1px;
      transform: translate(-50%, -50%);
      background: #e2e2e2; }
    .uploader-warp > .uploader-box > .uploader::after {
      width: 1px;
      height: 100rpx; }
  .uploader-warp > .uploader-box > .preview {
    display: inline-block;
    width: 160rpx;
    height: 160rpx; }
.uploader-warp > .uploader-btn {
  margin: 0 auto;
  border-radius: 10rpx;
  width: 500rpx;
  padding: 30rpx;
  text-align: center;
  background: #C0C4CC;
  color: #ffffff; }
  .uploader-warp > .uploader-btn.activate {
    background: #24a623; }
