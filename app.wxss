.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

@font-face {
  font-family: 'iconfont';  /* project id 1378954 */
  src: url('//at.alicdn.com/t/font_1378954_iwp8pszf1g.eot');
