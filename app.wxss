  url('//at.alicdn.com/t/font_1378954_iwp8pszf1g.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1378954_iwp8pszf1g.woff') format('woff'),
  url('//at.alicdn.com/t/font_1378954_iwp8pszf1g.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1378954_iwp8pszf1g.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
